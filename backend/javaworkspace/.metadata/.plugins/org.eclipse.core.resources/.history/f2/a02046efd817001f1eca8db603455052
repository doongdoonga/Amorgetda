package org.Amorgetda.day04.control.loop.exercise;

public class Exercise_fordouble01 {
	
	public static void main(String [] args) {
		// *
		// **
		// ***
		// ****
		// *****
		// ******
		
		for(int i = 1; i <= 6; i++) {
			System.out.println("*");
		}
		
		
		
		
		
		
		
		System.out.print("*");
		System.out.println();
	}
}