package org.Amorgetda.day04.control.loop.exercise;

public class Exercise_fordouble03 {

	public static void main(String[] args) {
		// 2단을 세로로 출력, 그 옆에 3단을 세로로 출력
		// 그 옆에 4단을 세로로 출력
		
		for(int j = 2; j <= 4; j++) {
			for(int i = 2; i < 10 ; i++) {
				System.out.print( j +" * " + i + " = " + j*i);
				System.out.println();
			}
			System.out.print("\n");
		}
	}

}
